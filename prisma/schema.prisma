generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://woo:1234@localhost:3306/ucworks"
}

model User {
  id       Int    @id @default(autoincrement()) // SERIAL을 사용하므로 autoincrement
  username String
  age      Int
}
